{
  "DiskProvisioning": "thin",
  "MarkAsTemplate": false,
  "Name": "{{.VMName}}",
  "IPAllocationPolicy": "fixedPolicy",
  "IPProtocol": "IPv4",
  "NetworkMapping": [
    {
      "Name": "custom",
      "Network": "/canada-dc/network/winnipeg"
    }
  ],
  "PropertyMapping": [
    {
      "Key": "ip0",
      "Value": "{{.TargetIP}}"
    },
    {
      "Key": "cidr",
      "Value": "{{.SubnetMask}}"
    },
    {
      "Key": "gateway",
      "Value": "{{.NetworkGateway}}"
    },
    {
      "Key":"DNS",
      "Value":"8.8.8.8"
    }
  ],
  "PowerOn": true,
  "InjectOvfEnv": false,
  "WaitForIP": false
}